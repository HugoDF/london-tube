<div class="card">
    <h1>London Tube</h1>
    <div><a href="/" class="logo">&#x1F687;</a></div>
    {{{slackbtn}}}
    {{#if teams.length}}
    <h2>{{teams.length}} active teams</h2>
    {{else}}
    <h2 class="inactive">No active teams yet</h2>
    {{/if}}
    <div class="description">
        <p>London Underground status information straight from TFL in one command.</p>
        <p>Just <code>/tube</code></p>
    </div>
    <img style="max-width: 100%" src="/images/demo.gif"/>
    <div>
        <a class="twitter-share-button"
      href="https://twitter.com/intent/tweet?text=London%20Tube%20status%20in%20Slack!&url=https%3A%2F%2Ftube.awebots.com&via=hugo__df">
    Tweet</a>
    </div>
    <script>window.twttr = (function(d, s, id) { var js, fjs =
d.getElementsByTagName(s)[0], t = window.twttr || {}; if (d.getElementById(id))
return t; js = d.createElement(s); js.id = id; js.src =
"https://platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs);
  t._e = []; t.ready = function(f) { t._e.push(f); };
  return t; }(document, "script", "twitter-wjs"));</script>
</div>
